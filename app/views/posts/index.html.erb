<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Public date</th>
      <th>City</th>
      <th>Country</th>
      <th>Gps</th>
      <th>Pictures</th>
      <th>Files</th>
      <th>Open or closed</th>
      <th>Solved</th>
      <th>Visible</th>
      <th>Dumpster</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.description %></td>
        <td><%= post.public_date %></td>
        <td><%= post.city %></td>
        <td><%= post.country %></td>
        <td><%= post.gps %></td>
        <td><%= post.pictures %></td>
        <td><%= post.files %></td>
        <td><%= post.open_or_closed %></td>
        <td><%= post.solved %></td>
        <td><%= post.visible %></td>
        <td><%= post.dumpster %></td>
        <td><%= post.user %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
